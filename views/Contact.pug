extends  base.pug    
//- this means i am filling the template base.pug  

block scripts
  script(src='../static/index.js')


block style
    style   
        include ../static/style.css
        include ../static/stylecontact.css

block content 
    h1(class="primary-h center") Contact Us
    form(action="/contact" method= "post" id="formaction") 
        input(type=" text" name="name" id="name" class="myinput" placeholder= "Enter Your name ")
        input(type="number" name="PhoneNo" id="PhoneNo" class="myinput" placeholder= "Enter Your PhoneNo.")
        input(type="email" name="Email" id="Email" class="myinput" placeholder= "Enter Your Email Id ")
        input(type="text" name="Address" id="Address" class="myinput" placeholder= "Enter Your Address ")
        textarea(name="concern", cols="30", rows="5" placeholder="Enter Your Concern" class="myinput") 
        input(type="submit" placeholder="submit"  class="myinput"  id="submitbtn")
        

